{
  "name": "sample-backend",
  "version": "1.1.0",
  "description": "Sample-Backend project serves as a template for an enterprise-level database wrapper service.",
  "main": "dit/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/username/repository.git"
  },
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "dev": "nodemon -r dotenv/config src/server.ts dotenv_config_path=./.env",
    "start": "set NODE_ENV=local&& ts-node -r dotenv/config src/server.ts dotenv_config_path=./.env",
    "production": "set NODE_ENV=production&& ts-node -r dotenv/config src/server.ts dotenv_config_path=./.env",
    "local": "set NODE_ENV=local&& ts-node -r dotenv/config src/server.ts dotenv_config_path=./.env",
    "test": "npm start&& newman run src/tests/sample-backend-tests.postman_collection.json -e src/tests/local.postman_environment.json"
  },
  "author": "Yoseph Solomon",
  "license": "MIT",
  "dependencies": {
    "@types/express-session": "^1.17.0",
    "@types/newman": "^5.1.1",
    "@types/passport": "^1.0.4",
    "@types/passport-http": "^0.3.8",
    "@types/uuid": "^8.3.0",
    "blerg": "0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "newman": "^5.2.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "pg-error": "^1.1.0",
    "pg-monitor": "^1.4.1",
    "pg-promise": "^9.2.1",
    "ts-dotenv": "^0.8.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.2",
    "nodemon": "^2.0.4",
    "tslint": "^6.1.3",
    "uuid": "^8.3.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  }
}
